{
  "keybindings": {
    "Home": {
      "<q>": {
        "action": "Quit",
        "displayName": "Quit"
      },
      "<Ctrl-d>": {
        "action": "Quit"
      },
      "<Ctrl-c>": {
        "action": "Quit"
      },
      "<Ctrl-z>": {
        "action": "Suspend"
      },
    },
    "Tab": {
      "displayGroups": {
        "scroll": {
          "displayName": "Scroll"
        },
        "tabs": {
          "displayName": "Select Tab"
        }
      },
      "extends": [
        "Home"
      ],
      "<c>": {
        "action": "Copy",
        "displayName": "Copy"
      },
      "<up>": {
        "action": "PrevItem",
        "displayGroup": "scroll"
      },
      "<down>": {
        "action": "NextItem",
        "displayGroup": "scroll"
      },
      "<right>": {
        "action": "Inspect",
        "displayName": "Inspect"
      },
      "<tab>": {
        "action": "NextTab",
        "displayName": "Tab"
      },
      "<f>": {
        "action": "Filter",
        "displayName": "Filter"
      },
      "<backtab>": {
        "action": "PrevTab"
      },
      "<enter>": {
        "action": "Enter",
      },
      "<esc>": {
        "action": "Escape",
      },
      "<1>": {
        "action": "Tab1",
        "displayGroup": "tabs"
      },
      "<2>": {
        "action": "Tab2",
        "displayGroup": "tabs"
      },
      "<3>": {
        "action": "Tab3",
        "displayGroup": "tabs"
      },
      "<4>": {
        "action": "Tab4",
        "displayGroup": "tabs"
      },
      "<5>": {
        "action": "Tab5",
        "displayGroup": "tabs"
      },
      "<6>": {
        "action": "Tab6",
        "displayGroup": "tabs"
      },
      "<7>": {
        "action": "Tab7",
        "displayGroup": "tabs"
      }
    },
    "FullTab": {
      "extends": [
        "Tab"
      ],
      "<n>": {
        "action": "NewItem",
        "displayName": "New"
      },
      "<d>": {
        "action": "DeleteItem",
        "displayName": "Delete"
      },
      "<r>": {
        "action": "LoadAllItems",
        "displayName": "Load"
      },
    },
    "ReadDeleteTab": {
      "extends": [
        "Tab"
      ],
      "<d>": {
        "action": "DeleteItem",
        "displayName": "Delete"
      },
      "<r>": {
        "action": "LoadAllItems",
        "displayName": "Load"
      }
    },
    "ReadTab": {
      "extends": [
        "Tab"
      ],
      "<r>": {
        "action": "LoadAllItems",
        "displayName": "Load"
      }
    },
    "BackupTab": {
      "extends": [
        "ReadDeleteTab"
      ],
      "<Ctrl-s>": {
        "action": "StartBackup",
        "displayName": "Start Backup"
      }
    },
    "EditorReadOnly": {
      "<Ctrl-d>": {
        "action": "Quit",
      },
      "<Ctrl-c>": {
        "action": "Quit",
        "displayName": "Quit"
      },
      "<Ctrl-z>": {
        "action": "Suspend",
      },
      "<esc>": {
        "action": "Escape",
        "displayName": "Cancel"
      }
    },
    "CreateEditor": {
      "extends": [
        "EditorReadOnly"
      ],
      "<Ctrl-n>": {
        "action": "CreateItem",
        "displayName": "Create"
      }
    },
    "UpdateEditor": {
      "extends": [
        "EditorReadOnly"
      ],
      "<Ctrl-n>": {
        "action": "UpdateItem",
        "displayName": "Update"
      }
    },
    "ErrorPopup": {
      "extends": [
        "Home"
      ],
      "<esc>": {
        "action": "ClosePopup",
        "displayName": "Close"
      }
    },
    "ConfirmPopup": {
      "extends": [
        "Home"
      ],
      "<esc>": {
        "action": "ClosePopup",
        "displayName": "Cancel"
      },
      "<enter>": {
        "action": "ConfirmPopup",
        "displayName": "Confirm"
      },
    },
    "FilterPopup": {
      "extends": [
        "EditorReadOnly"
      ],
      "<esc>": {
        "action": "ClosePopup",
        "displayName": "Cancel"
      },
      "<enter>": {
        "action": "ConfirmPopup",
        "displayName": "Confirm Filter"
      },
    }
  }
}
